{
    "name": "rc/phpcr-route-events-bundle",
    "type": "symfony-bundle",
    "description": "Events in PHPCR Route class",
    "keywords": ["phpcr", "event", "route", "bundle"],
    "homepage": "https://github.com/rafaelcalleja/PHPCRRouteEventsBundle.git", 
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Calleja",
            "email": "rafa.calleja@d-noise.net"
        }
    ],
    "require": {
        "php": ">=5.3.2",
	"symfony-cmf/routing-extra-bundle": "dev-master"
    },
    "require-dev": {
        "symfony/class-loader": ">=2.1,<2.3-dev",
        "doctrine/doctrine-bundle": "1.*",
        "symfony/yaml": ">=2.1,<2.3-dev",
        "symfony/form": ">=2.1,<2.3-dev",
        "symfony/finder": ">=2.1,<2.3-dev",
        "jackalope/jackalope-doctrine-dbal": "1.0.*",
        "doctrine/phpcr-odm": "1.0.*",
        "doctrine/phpcr-bundle": "1.0.*",
	"doctrine/common": "dev-master as 2.3.x-dev",
	"symfony-cmf/routing-extra-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "RC\\PHPCRRouteEventsBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "RC/PHPCRRouteEventsBundle"
}
